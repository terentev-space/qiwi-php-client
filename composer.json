{
  "name": "terentev-space/qiwi-php-client",
  "type": "library",
  "version": "0.1.1",
  "description": "Universal payments API SDK (UNOFICIAL QIWI CLIENT)",
  "license": "MIT",
  "keywords": [
    "qiwi",
    "api",
    "php",
    "client"
  ],
  "homepage": "https://github.com/terentev-space/qiwi-php-client",
  "authors": [
    {
      "name": "Ivan Terentev"
    }
  ],
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^6.3",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/log": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "QiwiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "QiwiClient\\Test\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "master"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^8.1"
  }
}
